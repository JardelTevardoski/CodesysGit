 5000000 THEN Pulse_10s:= TRUE; ELSE Pulse_10s:= FALSE; END_IF\nIF udiMicroAcc MOD 30000000 >= 15000000 THEN Pulse_30s:= TRUE; ELSE Pulse_30s:= FALSE; END_IF\nIF udiMicroAcc MOD 60000000 >= 30000000 THEN Pulse_60s:= TRUE; ELSE Pulse_60s:= FALSE; END_IF\nudiMicroAcc:= udiMicroAcc MOD 120000000;\n\n// Update scan pulse\nPulse_Scan:= NOT Pulse_Scan;\n\n// Update system time\nGetDateAndTime(System_Time);\n\n// Update Hour_Minute\nHour_Minute:= System_Time.byHours * 100 + System_Time.byMinutes;\n\n// Update Day of Week\nDay_Of_Week:= NextoStandard.GetDayOfWeek();","LineInfoPersistence":"(string)d755fd50-40aa-4b7c-b7aa-e8ecb415c006_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)(*This POU is blocked to edit.*)\n{attribute 'hide'}\nPROGRAM SpecialVariablesPrg\nVAR\n\tudiMicroAcc:\tUDINT;\t\t\t// Microseconds accumulator\n\tuliSystemTime:\tULINT;\t\t\t// System time of this cycle [us]\n\tuliLastSysTime:\tULINT;\t\t\t// System time of last cycle [us]\nEND_VAR","LineInfoPersistence":"(string)d755fd50-40aa-4b7c-b7aa-e8ecb415c006_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)37","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}